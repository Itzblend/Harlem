{
    "version":1,
    "root":{
        "handlers" : ["console"],
        "level": "DEBUG"
    },
    "handlers":{
        "console":{
            "formatter": "std_out",
            "class": "logging.StreamHandler",
            "level": "DEBUG"
        }
    },
    "formatters":{
        "std_out": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(filename)-12s Line: %(lineno)-6s %(funcName)s - %(message)s",
            "datefmt":"%d-%m-%Y %I:%M:%S"
        }
    }
}